{
  "name": "@ribajs/doc",
  "version": "2.0.0-alpha.7",
  "description": "Shopify theme for the Riba.js Documentation",
  "type": "module",
  "exports": "./src/ts/main.ts",
  "main": "./src/ts/main.ts",
  "typings": "src/ts/types",
  "private": true,
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "deploy": "yarn run deploy:pug && yarn run deploy:gulp",
    "deploy:gulp": "gulp deploy --gulpfile gulpfile.cjs --environment=production",
    "deploy:pug": "yarn run build:prod:pug && theme deploy --dir ./theme layout sections snippets templates",
    "sync:demos": "rm -rf ./src/ts/demos/ && rsync -av --progress ../../demos ./src/ts/ --exclude .cache --exclude .yarn --exclude dist --exclude .eslintrc.js",
    "lint:ts": "eslint ./src --ext .ts,.tsx --fix && tsc --noEmit",
    "lint:gulp": "gulp lint --gulpfile gulpfile.cjs",
    "lint": "yarn run sync:demos && yarn run lint:ts && yarn run lint:gulp",
    "build": "yarn run sync:demos && yarn run build:prod:pug && gulp build --gulpfile gulpfile.cjs --environment=production",
    "build:prod:pug": "concurrently \"yarn:build:prod:pug:*\"",
    "build:prod:pug:snippets": "pug -O ./pug.options.cjs src/pug/snippets -E liquid -o theme/snippets",
    "build:prod:pug:templates": "pug -O ./pug.options.cjs src/pug/templates -E liquid -o theme/templates",
    "build:prod:pug:layout": "pug -O ./pug.options.cjs src/pug/layout -E liquid -o theme/layout",
    "build:dev": "yarn run sync:demos && concurrently \"yarn:build:dev:*\"",
    "build:dev:pug": "concurrently \"yarn:build:dev:pug:*\"",
    "build:dev:pug:snippets": "yarn run build:prod:pug:snippets --pretty",
    "build:dev:pug:templates": "yarn run build:prod:pug:templates --pretty",
    "build:dev:pug:layout": "yarn run build:prod:pug:layout --pretty",
    "zip": "gulp zip --gulpfile gulpfile.cjs --environment=production",
    "watch": "concurrently \"yarn:watch:*\"",
    "watch:gulp": "gulp watch --gulpfile gulpfile.cjs --environment=development",
    "watch:pug": "concurrently \"yarn:watch:pug:*\"",
    "watch:pug:snippets": "yarn run build:dev:pug:snippets --watch",
    "watch:pug:templates": "yarn run build:dev:pug:templates --watch",
    "watch:pug:layout": "yarn run build:dev:pug:layout --watch",
    "themes:list:live": "gulp themes:list:live --gulpfile gulpfile.cjs",
    "themes:list:oldest": "gulp themes:list:oldest --gulpfile gulpfile.cjs",
    "themes:list:youngest": "gulp themes:list:youngest --gulpfile gulpfile.cjs",
    "generate:config:live": "gulp generate:config:live --gulpfile gulpfile.cjs",
    "generate:config:deploy": "gulp generate:config:deploy --gulpfile gulpfile.cjs",
    "assets:list": "gulp assets:list --gulpfile gulpfile.cjs",
    "assets:update": "gulp assets:update --gulpfile gulpfile.cjs",
    "backup:locale": "gulp backup:locale --gulpfile gulpfile.cjs",
    "backup:settings_data": "gulp backup:settings_data --gulpfile gulpfile.cjs",
    "release:new": "gulp release:new --gulpfile gulpfile.cjs",
    "release:zip": "gulp release:zip --gulpfile gulpfile.cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/doc"
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap4",
    "Shopify",
    "Art+Code Studio",
    "Riba",
    "Rivets",
    "tinybind",
    "Documentation",
    "Docs"
  ],
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/doc/blob/master/README.md"
    }
  ],
  "bugs": {
    "url": "https://github.com/ribajs/doc/issues"
  },
  "url": "https://github.com/ribajs/doc",
  "homepage": "https://ribajs.com/",
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
