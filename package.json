    "prismjs": "^1.28.0",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.4.0",
    "sass": "^1.53.0",
    "sass-loader": "^12.6.0",
    "semver": "^7.3.7",
    "shelljs": "^0.8.5",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.1",
    "typescript": "4.7.4",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^4.0.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-stream": "^7.0.0",
    "yargs": "^17.5.1"
  },
  "dependencies": {
    "@ribajs/bs4": "workspace:^",
    "@ribajs/core": "workspace:^",
    "@ribajs/events": "workspace:^",
    "@ribajs/extras": "workspace:^",
