    "config-chain": "^1.1.12",
    "core-js": "^3.6.5",
    "css-flip": "^0.5.0",
    "css-loader": "^4.3.0",
    "csso-cli": "^3.0.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "extract-loader": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-print": "^5.0.2",
    "gulp-zip": "^5.0.2",
    "has-unicode": "^2.0.1",
    "html-loader": "^1.3.1",
    "inherits": "^2.0.4",
    "ini": "^1.3.5",
    "inspectpack": "^4.5.2",
    "jshint": "^2.12.0",
    "jstransformer-escape-html": "^1.1.0",
    "jstransformer-markdown-it": "^2.1.0",
    "jstransformer-prismjs": "^0.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "postcss": "^7.0.35",
    "postcss-cli": "^7.1.2",
    "postcss-loader": "^3.0.0",
