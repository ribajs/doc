{
  "name": "@ribajs/doc",
  "version": "1.9.0",
  "description": "Shopify theme for the Riba.js Documentation",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "private": true,
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "webpack": "webpack --progress --color --env production",
    "webpack:dev": "webpack --progress --color --env development",
    "deploy": "yarn run build && theme deploy --dir ./theme",
    "deploy:wp": "yarn run build:prod:wp && theme deploy --dir ./theme assets/main.bundle.js assets/vendors.bundle.js assets/main.css",
    "deploy:pug": "yarn run build:prod:pug && theme deploy --dir ./theme layout sections snippets templates",
    "lint:ts": "eslint src --ext .ts,.tsx --fix && tsc --noEmit",
    "lint": "",
    "sync:demos": "rm -rf ./src/ts/demos/ && rsync -av --progress ../../demos ./src/ts/ --exclude .cache --exclude .yarn --exclude dist --exclude .eslintrc.js",
    "build": "yarn run sync:demos && concurrently \"yarn:build:prod:*\"",
    "build:prod:wp": "yarn run webpack",
    "build:prod:pug": "concurrently \"yarn:build:prod:pug:*\"",
    "build:prod:pug:snippets": "pug -O ./pug.options.js src/pug/snippets -E liquid -o theme/snippets",
    "build:prod:pug:templates": "pug -O ./pug.options.js src/pug/templates -E liquid -o theme/templates",
    "build:prod:pug:layout": "pug -O ./pug.options.js src/pug/layout -E liquid -o theme/layout",
    "build:prod:theme_settings": "gulp build:theme_settings",
    "build:prod:assets": "gulp build:assets:custom && gulp build:assets",
    "build:prod:assets:iconset": "gulp build:assets:iconset",
    "build:prod:assets:favicons": "gulp build:assets:favicons",
    "build:dev": "yarn run sync:demos && concurrently \"yarn:build:dev:*\"",
    "build:dev:wp": "yarn run webpack:dev",
    "build:dev:pug": "concurrently \"yarn:build:dev:pug:*\"",
    "build:dev:pug:snippets": "yarn run build:prod:pug:snippets --pretty",
    "build:dev:pug:templates": "yarn run build:prod:pug:templates --pretty",
    "build:dev:pug:layout": "yarn run build:prod:pug:layout --pretty",
    "build:dev:theme_settings": "gulp build:theme_settings",
    "build:dev:assets": "gulp build:assets:custom && gulp:build:assets",
    "build:dev:assets:iconset": "gulp build:assets:iconset",
    "build:dev:assets:favicons": "gulp build:assets:favicons",
    "zip": "gulp build:zip",
    "watch": "concurrently \"yarn:watch:*\"",
    "watch:theme": "theme watch --dir './theme'",
    "watch:wp": "yarn run webpack:dev --watch",
    "watch:pug": "concurrently \"yarn:watch:pug:*\"",
    "watch:pug:snippets": "yarn run build:dev:pug:snippets --watch",
    "watch:pug:templates": "yarn run build:dev:pug:templates --watch",
    "watch:pug:layout": "yarn run build:dev:pug:layout --watch",
    "checkout": "concurrently -k \"yarn:checkout:*\"",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "packages:yarn:link": "yarn link @ribajs/core @ribajs/i18n @ribajs/bs4 @ribajs/shopify @ribajs/router @ribajs/shopify-tda @ribajs/iconset @ribajs/extras"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/doc"
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap4",
    "Shopify",
    "Art+Code Studio",
    "Riba",
    "Rivets",
    "tinybind",
    "Documentation",
    "Docs"
  ],
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/doc/blob/master/README.md"
    }
  ],
  "bugs": {
    "url": "https://github.com/ribajs/doc/issues"
  },
  "url": "https://github.com/ribajs/doc",
  "homepage": "https://ribajs.com/",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-export-default-from": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.8",
    "@babel/runtime-corejs3": "^7.14.8",
    "@babel/types": "^7.14.8",
    "@ribajs/babel-config": "workspace:infra/babel-config",
    "@ribajs/eslint-config": "workspace:*",
    "@ribajs/postcss-config": "workspace:infra/postcss-config",
    "@ribajs/tsconfig": "workspace:*",
    "@ribajs/webpack-config": "workspace:infra/webpack-config",
    "@types/debug": "^4.1.7",
    "@types/prismjs": "^1.16.6",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@yarnpkg/pnpify": "^3.0.0-rc.14",
    "abbrev": "^1.1.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babelify": "^10.0.0",
    "clean-css-cli": "^5.3.0",
    "concurrently": "^6.2.0",
    "config-chain": "^1.1.13",
    "core-js": "^3.15.2",
    "css-flip": "^0.5.0",
    "css-loader": "^6.2.0",
    "csso-cli": "^3.0.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "extract-loader": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-plumber": "^1.2.1",
    "gulp-print": "^5.0.2",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.1.0",
    "has-unicode": "^2.0.1",
    "html-loader": "^2.1.2",
    "inherits": "^2.0.4",
    "ini": "^1.3.8",
    "inspectpack": "^4.7.1",
    "jshint": "^2.13.0",
    "jstransformer-escape-html": "^1.1.0",
    "jstransformer-markdown-it": "^2.1.0",
    "jstransformer-prismjs": "^0.1.0",
    "mini-css-extract-plugin": "^2.1.0",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^4.3.0",
    "postcss-modules": "^4.2.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "prismjs": "^1.24.1",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.4.0",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "semver": "^7.3.5",
    "shelljs": "^0.8.4",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.4",
    "typescript": "^4.3.5",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^4.0.0",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-stream": "^6.1.2",
    "yargs": "^17.0.1"
  },
  "dependencies": {
    "@ribajs/bs4": "workspace:*",
    "@ribajs/core": "workspace:*",
    "@ribajs/extras": "workspace:*",
    "@ribajs/i18n": "workspace:*",
    "@ribajs/iconset": "workspace:*",
    "@ribajs/photoswipe": "workspace:*",
    "@ribajs/router": "workspace:*",
    "@ribajs/shopify": "workspace:*",
    "@ribajs/shopify-tda": "workspace:*",
    "@ribajs/utils": "workspace:*",
    "bootstrap": "^4.6.0",
    "debug": "^4.3.2",
    "gulp-cheerio": "^1.0.0",
    "gulp-ext-replace": "^0.3.0",
    "gulp-include": "^2.4.1",
    "gulp-rename": "^2.0.0",
    "gulp-size": "^4.0.1",
    "gulp-svgmin": "^4.0.1",
    "jquery": "^3.6.0",
    "lorem-ipsum": "^2.0.3",
    "popper.js": "^1.16.1",
    "require-dir": "^1.2.0",
    "semver": "^7.3.5",
    "three": "^0.130.1"
  }
}
