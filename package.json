    "typescript": "^3.9.7",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^5.0.0-rc.4",
    "webpack-cli": "^3.3.12",
    "webpack-stream": "^5.2.1",
    "yargs": "^15.4.1"
  },
  "dependencies": {
    "@ribajs/bs4": "^1.9.0-alpha.2",
    "@ribajs/core": "^1.9.0-alpha.2",
    "@ribajs/extras": "^1.9.0-alpha.2",
    "@ribajs/i18n": "^1.9.0-alpha.2",
    "@ribajs/iconset": "^1.9.0-alpha.2",
    "@ribajs/photoswipe": "^1.9.0-alpha.2",
