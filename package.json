{
  "name": "@ribajs/doc",
  "version": "1.9.0-alpha.2",
  "description": "Shopify theme for the Riba.js Documentation",
  "main": "src/ts/main.ts",
  "typings": "src/ts/types",
  "scripts": {
    "theme:download": "theme download --dir './theme'",
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development",
    "deploy": "npm run build && theme deploy --dir ./theme",
    "deploy:wp": "npm run build:prod:wp && theme deploy --dir ./theme assets/main.bundle.js assets/vendors.bundle.js assets/main.css",
    "deploy:pug": "npm run build:prod:pug && theme deploy --dir ./theme layout sections snippets templates",
    "lint:ts": "eslint src --ext .ts,.tsx --fix && tsc --noEmit",
    "lint": "npm run lint:ts",
    "build": "concurrently \"npm:build:prod:*\"",
    "build:prod:demos": "rm -rf ./src/ts/demos/ && rsync -av --progress ../../demos ./src/ts/ --exclude .cache --exclude .yarn --exclude dist --exclude .eslintrc.js",
    "build:prod:wp": "npm run webpack",
    "build:prod:pug": "concurrently \"npm:build:prod:pug:*\"",
    "build:prod:pug:snippets": "pug -O ./pug.options.js src/pug/snippets -E liquid -o theme/snippets",
    "build:prod:pug:templates": "pug -O ./pug.options.js src/pug/templates -E liquid -o theme/templates",
    "build:prod:pug:layout": "pug -O ./pug.options.js src/pug/layout -E liquid -o theme/layout",
    "build:prod:theme_settings": "gulp build:theme_settings",
    "build:prod:assets": "gulp build:assets",
    "build:prod:assets:iconset": "gulp build:assets:iconset",
    "build:prod:assets:favicons": "gulp build:assets:favicons",
    "build:dev": "concurrently \"npm:build:dev:*\"",
    "build:dev:demos": "rm -rf ./src/ts/demos/ && rsync -av --progress ../../demos ./src/ts/ --exclude .cache --exclude .yarn --exclude dist --exclude .eslintrc.js",
    "build:dev:ts": "npm run webpack:dev",
    "build:dev:pug": "concurrently \"npm:build:dev:pug:*\"",
    "build:dev:pug:snippets": "npm run build:prod:pug:snippets -- --pretty",
    "build:dev:pug:templates": "npm run build:prod:pug:templates -- --pretty",
    "build:dev:pug:layout": "npm run build:prod:pug:layout -- --pretty",
    "build:dev:theme_settings": "gulp build:theme_settings",
    "build:dev:assets": "gulp build:assets",
    "build:dev:assets:iconset": "gulp build:assets:iconset",
    "build:dev:assets:favicons": "gulp build:assets:favicons",
    "zip": "gulp build:zip",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:theme": "theme watch --dir './theme'",
    "watch:wp": "npm run webpack:dev -- --watch",
    "watch:pug": "concurrently \"npm:watch:pug:*\"",
    "watch:pug:snippets": "npm run build:dev:pug:snippets -- --watch",
    "watch:pug:templates": "npm run build:dev:pug:templates -- --watch",
    "watch:pug:layout": "npm run build:dev:pug:layout -- --watch",
    "checkout": "concurrently -k \"npm:checkout:*\"",
    "checkout:js": "uglifyjs checkout/custom-checkout.js -o checkout/custom-checkout.min.js -c -m;",
    "checkout:css": "cleancss checkout/custom-checkout.css -o checkout/custom-checkout.min.css",
    "packages:npm:link": "npm link @ribajs/core @ribajs/i18n @ribajs/bs4 @ribajs/shopify @ribajs/router @ribajs/shopify-tda @ribajs/iconset @ribajs/extras"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ribajs/doc"
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap4",
    "Shopify",
    "Art+Code Studio",
    "Riba",
    "Rivets",
    "tinybind",
    "Documentation",
    "Docs"
  ],
  "author": "Pascal Garber <pascal@artandcode.studio>",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ribajs/doc/blob/master/README.md"
    }
  ],
  "bugs": {
    "url": "https://github.com/ribajs/doc/issues"
  },
  "url": "https://github.com/ribajs/doc",
  "homepage": "https://ribajs.com/",
