    "@ribajs/router": "workspace:^",
    "@ribajs/shopify": "workspace:^",
    "@ribajs/shopify-tda": "workspace:^",
    "@ribajs/utils": "workspace:^",
    "bootstrap": "^4.6.2",
    "debug": "^4.3.4",
    "gulp-cheerio": "^1.0.0",
    "gulp-ext-replace": "^0.3.0",
    "gulp-include": "^2.4.1",
    "gulp-rename": "^2.0.0",
    "gulp-size": "^4.0.1",
    "gulp-svgmin": "^4.1.0",
    "jquery": "^3.6.0",
    "lorem-ipsum": "^2.0.8",
    "popper.js": "^1.16.1",
