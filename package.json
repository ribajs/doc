    "@ribajs/utils": "^1.9.0-alpha.2",
    "bootstrap": "^4.5.2",
    "debug": "^4.3.0",
    "jquery": "^3.5.1",
    "lorem-ipsum": "^2.0.3",
    "popper.js": "^1.16.1",
    "three": "^0.121.1"
  },
  "resolutions": {
    "webpack": "5.0.0-beta.22",
    "terser-webpack-plugin": "3.0.7",
    "ribajs": "portal:../..",
    "@ribajs/bs4": "portal:../../packages/bs4",
