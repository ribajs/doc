    "babelify": "^10.0.0",
    "clean-css-cli": "^5.6.1",
    "concurrently": "^7.3.0",
    "config-chain": "^1.1.13",
    "core-js": "^3.23.5",
    "css-flip": "^0.5.0",
    "css-loader": "^6.7.1",
    "csso-cli": "^3.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "extract-loader": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-plumber": "^1.2.1",
    "gulp-print": "^5.0.2",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.1.0",
    "has-unicode": "^2.0.1",
    "html-loader": "^3.1.2",
    "inherits": "^2.0.4",
    "ini": "^2.0.0",
    "inspectpack": "^4.7.1",
    "jshint": "^2.13.5",
    "jstransformer-escape-html": "^1.1.0",
    "jstransformer-markdown-it": "^2.1.0",
    "jstransformer-prismjs": "^0.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-modules": "^4.3.1",
